{
  "name": "invoicer",
  "version": "0.0.1",
  "main": "main.js",
  "config": {
    "app_name": "Invoicer",
    "dist_dir": "./dist",
    "electron_dist_dir": "./dist/electron"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200 --disable-host-check",
    "serve:ssr": "node $npm_package_config_dist_dir/server",
    "serve:electron": "npm run build:electron && electron .",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:client-and-server-bundles": "ng build --prod && ng run $npm_package_name:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:electron": "tsc main.ts && ng build --prod --base-href ./ && cp package.json $npm_package_config_dist_dir/browser && cp main.js $npm_package_config_dist_dir/browser/",
    "pkg:electron:all": "npm run build:electron && electron-packager $npm_package_config_dist_dir/browser $npm_package_config_app_name --all --asar",
    "pkg:electron:local": "npm run build:electron && electron-packager . $npm_package_config_app_name --asar=true --icon=src/assets/icons/icon.icns --prune=true --out=$npm_package_config_electron_dist_dir --overwrite",
    "pkg:electron:mac": "npm run build:electron && electron-packager . $npm_package_config_app_name --asar=true --icon=src/assets/icons/icon.icns --prune=true --out=$npm_package_config_electron_dist_dir --overwrite --platform=darwin --arch=x64",
    "pkg:electron:windows": "npm run build:electron && electron-packager . $npm_package_config_app_name --asar=true --icon=src/assets/icons/icon.icns --prune=true --out=$npm_package_config_electron_dist_dir --overwrite --platform=win32 --arch=ia32 --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"$npm_package_config_app_name\"",
    "pkg:electron:linux": "npm run build:electron && electron-packager . $npm_package_config_app_name --asar=true --icon=src/assets/icons/icon.icns --prune=true --out=$npm_package_config_electron_dist_dir --overwrite --platform=linux --arch=x64",
    "build:electron:installer::mac": "npm run pkg:electron:mac && electron-installer-dmg $npm_package_config_electron_dist_dir/$npm_package_config_app_name-darwin-x64/$npm_package_config_app_name.app $npm_package_config_app_name --out=$npm_package_config_electron_dist_dir --overwrite",
    "build:electron:installer::windows": "npm run pkg:electron:mac && electron-installer-dmg $npm_package_config_electron_dist_dir/$npm_package_config_app_name-darwin-x64/$npm_package_config_app_name.app $npm_package_config_app_name --out=$npm_package_config_electron_dist_dir --overwrite",
    "gh-publish": "ng build --prod --base-href https://dhavalpowar.github.io/$npm_package_name/ && ngh --dir $npm_package_config_dist_dir/browser",
    "compodoc": "compodoc -p src/tsconfig.app.json -n '$npm_package_config_app_name'",
    "compodoc:serve": "compodoc -p src/tsconfig.app.json -n '$npm_package_config_app_name' -s",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/cdk": "^6.4.3",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/forms": "^6.1.2",
    "@angular/http": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/platform-server": "^6.1.2",
    "@angular/pwa": "^0.7.3",
    "@angular/router": "^6.1.2",
    "@angular/service-worker": "^6.1.2",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/hapi": "^17.0.17",
    "@types/spdy": "^3.4.4",
    "compression": "^1.7.3",
    "core-js": "^2.5.4",
    "domino": "^2.1.0",
    "ngrx-store-freeze": "^0.2.4",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.1.0",
    "spdy": "^3.4.7",
    "ts-loader": "^4.4.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/language-service": "^6.1.2",
    "@compodoc/compodoc": "^1.1.3",
    "@ngrx/schematics": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.2.1",
    "copy-webpack-plugin": "^4.5.2",
    "electron": "^2.0.7",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.6.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.1.0"
  }
}
